 <ContactContext.Provider\n            value = {{\n                contacts: state.contacts,\n                current: state.current,\n                addContact,\n                deleteContact,\n                setCurrent,\n                clearCurrent,\n            }}\n        >\n            \n            { props.children }\n        </ContactContext.Provider>\n    )\n}\n\nexport default ContactState;","/home/dorin/Desktop/React/Contact-Keeper/client/src/context/contacts/contactReducer.js",["61","62","63"],"import {\n    ADD_CONTACT,\n    DELETE_CONTACT,\n    SET_CURRENT,\n    CLEAR_CURRENT,\n    UPDATE_CONTACTS,\n    FILTER_CONTACTS,\n    CLEAR_FILTER\n} from '../types'\n\nconst contactReducer = (state, action) => {\n    switch(action.type){\n        case ADD_CONTACT: \n            return {\n                ...state,\n                contacts: [...state.contacts, action.payload]\n            }\n        case DELETE_CONTACT: \n            return {\n                ...state,\n                contacts: state.contacts.filter(contact => contact.id !== action.payload),\n            }\n        case SET_CURRENT: \n            return {\n                ...state,\n                current: action.payload,\n            }\n        case CLEAR_CURRENT: \n            return {\n                ...state,\n                current: null\n            }\n\n        default: \n            return state;\n    }\n}\n\nexport default contactReducer;","/home/dorin/Desktop/React/Contact-Keeper/client/src/context/contacts/contactContext.js",[],{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","severity":1,"message":"69","line":1,"column":39,"nodeType":"70","messageId":"71","endLine":1,"endColumn":48},{"ruleId":"68","severity":1,"message":"72","line":7,"column":25,"nodeType":"70","messageId":"71","endLine":7,"endColumn":32},{"ruleId":"68","severity":1,"message":"73","line":10,"column":5,"nodeType":"70","messageId":"71","endLine":10,"endColumn":20},{"ruleId":"68","